{
  "functions": {
    "ignore": [
      "node_modules",
      ".git",
      "firebase-debug.log",
      "firebase-debug.*.log"
    ],
    "predeploy": [
      "pnpm run lint",
      "pnpm run build"
    ],
    "source": "."
  },
  "emulators": {
    "functions": {
      "port": 5002
    },
    "firestore": {
      "port": 8081
    },
    "ui": {
      "enabled": true
    }
  }
}
