    "taze": "^0.7.6",
    "typescript": "^4.8.4",
    "unocss": "^0.45.5",
    "unplugin-auto-import": "^0.11.0",
    "unplugin-icons": "^0.14.10",
    "unplugin-vue-components": "^0.22.0",
    "vite": "^3.0.4",
