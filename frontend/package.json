    "ky": "^0.31.3",
    "ky-universal": "^0.10.1",
    "naive-ui": "^2.33.3",
    "pinia": "^2.0.17",
    "vue": "^3.2.39",
    "vue-demi": "^0.13.6",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.25.2",
    "@iconify-json/carbon": "^1.1.8",
    "@iconify-json/flag": "^1.1.20",
    "@intlify/vite-plugin-vue-i18n": "^6.0.0",
